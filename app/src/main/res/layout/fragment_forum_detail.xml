        <com.rayhahah.easysports.view.HuPuWebView
            android:id="@+id/wv_hupu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="?attr/colorBg"
            />

    </LinearLayout>

</layout>
